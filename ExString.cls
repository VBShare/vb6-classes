Public Function RepeatMe(ByVal txt As String, ByVal times As Integer) As String
  Dim i As Integer
  Dim tmpCopy As String
  tmpCopy = ""
  If times > 0 Then
    For i = 1 To times
      tmpCopy = tmpCopy & txt
    Next i
  End If
  RepeatMe = tmpCopy
End Function
